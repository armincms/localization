{
    "name": "armincms/localization",
    "description": "The Armin Cms Localization Package.",
    "keywords": [
        "laravel",
        "nova",
        "armincms",
        "localization"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0" 
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Localization\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Localization\\LocalizationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
